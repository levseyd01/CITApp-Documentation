{
    "routes": [
        {
            "route": "/.auth/login/github",
            "statusCode": "401"
        },
        {
            "route": "/.auth/login/twitter",
            "statusCode": "401"
        },
        {
            "route": "/.auth/login/facebook",
            "statusCode": "401"
        },
        {
            "route": "/.auth/login/google",
            "statusCode": "401"
        },
        {
            "route": "/login",
            "serve": "/.auth/login/aad"
        },
        {
            "route": "/logout",
            "serve": "/.auth/logout"
        },
        {
            "route": "/",
            "allowedRoles": ["anonymous"]
        },
        {
            "route": "/authenticated/*",
            "allowedRoles": ["authenticated"]
        },
        {
            "route": "/reader_role/*",
            "allowedRoles": ["reader", "contributor"]
        }
    ]
}
