{
    "navigationFallback": {
      "rewrite": "index.html",
      "exclude": [
        "/images/*.{png,jpg,gif}",
        "/_static/*.js",
        "/_static/*.css"
      ]
    },
    "mimeTypes": {
      ".json": "text/json"
    },
    "globalHeaders": {
      "Cache-Control": "public, max-age=31536000"
    }
  }
  